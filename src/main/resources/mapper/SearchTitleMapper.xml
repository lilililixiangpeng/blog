<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="demo.web.mapper.TitleSearchMapper">
    <select id="searchtitle" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT title FROM title WHERE name = #{name}
    </select>
</mapper>